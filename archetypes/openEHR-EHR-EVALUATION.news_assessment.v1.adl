definition
	EVALUATION[at0000] matches {	-- NEWS utvärdering
		data matches {
			ITEM_TREE[at0001] matches {	-- *Tree(en)
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- NEWS utvärdering
						value matches {
							0|[local::at0004], 	-- Noll poäng
							1|[local::at0005], 	-- Låg risk
							2|[local::at0006], 	-- Medelhög risk
							3|[local::at0007]; 	-- Hög risk
							0  	-- assumed value
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {	-- Observation frekvens
						value matches {
							0|[local::at0008], 	-- Minimum två gånger per dygn
							1|[local::at0009], 	-- Minimum var 4-6:e timme
							2|[local::at0010], 	-- Minst en gång i timmen
							3|[local::at0011]; 	-- Kontinuerlig kontroll av vitala parametrar
							0  	-- assumed value
						}
					}
					ELEMENT[at0012] occurrences matches {0..1} matches {	-- Rekommendation
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0013, 	-- NEWS assessment vid varje rutin kontroll
									at0014, 	-- Obligatorisk bedömning av en legitimerad sjuksköterska om patienten behöver mer kontroll och/eller högre vård nivå
									at0015, 	-- Brådskande bedömning av en kompetent läkare och vård i avdelning med övervakningsutrustningar
									at0016; 	-- Akut bedömning av en kritisk vårdspecialist och överföring till intensivvårdsavdelning
									at0013]	-- assumed value
								}
							}
						}
					}
				}
			}
		}
	}

