definition
	OBSERVATION[at0000] matches {	-- AIR Score
		data matches {
			HISTORY[at0001] matches {	-- *Event Series(en)
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- *Any event(en)
						data matches {
							ITEM_TREE[at0003] matches {	-- *Tree(en)
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {	-- Kräkning
										value matches {
											0|[local::at0005], 	-- Nej
											1|[local::at0006]  	-- Ja
										}
									}
									ELEMENT[at0007] occurrences matches {0..1} matches {	-- Smärta höger fossa
										value matches {
											0|[local::at0005], 	-- Nej
											1|[local::at0006]  	-- Ja
										}
									}
									ELEMENT[at0008] occurrences matches {0..1} matches {	-- Släppömhet eller muskelförsvar
										value matches {
											0|[local::at0005], 	-- Nej
											1|[local::at0009], 	-- Lätt
											2|[local::at0010], 	-- Måttlig
											3|[local::at0011]  	-- Kraftig
										}
									}
									ELEMENT[at0012] occurrences matches {0..1} matches {	-- Kroppstemperatur >= 38.5 (°C)
										value matches {
											0|[local::at0005], 	-- Nej
											1|[local::at0006]  	-- Ja
										}
									}
									ELEMENT[at0013] occurrences matches {0..1} matches {	-- Neutrofiler (%)
										value matches {
											0|[local::at0014], 	-- < 70%
											1|[local::at0015], 	-- 70-84%
											2|[local::at0016]  	-- >= 85%
										}
									}
									ELEMENT[at0017] occurrences matches {0..1} matches {	-- Leukocyter (×10*9/L)
										value matches {
											0|[local::at0018], 	-- < 10
											1|[local::at0019], 	-- 10-14,9
											2|[local::at0020]  	-- >= 15
										}
									}
									ELEMENT[at0021] occurrences matches {0..1} matches {	-- CRP (mg/L)
										value matches {
											0|[local::at0018], 	-- < 10
											1|[local::at0022], 	-- 10-49
											2|[local::at0023]  	-- >= 50
										}
									}
									ELEMENT[at0024] occurrences matches {0..1} matches {	-- Poängsumma
										value matches {
											DV_COUNT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

