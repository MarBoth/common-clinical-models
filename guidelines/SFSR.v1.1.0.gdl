						description = <"HCT/PCV (Hematocrit/packed cell volume) %">
					>
					["gt0009"] = (TERM) <
						text = <"EKG abnormal">
						description = <"New EKG change from any source, any non-sinus rhythm on EKG or monitoring">
					>
					["gt0010"] = (TERM) <
						text = <"Shortness of Breath">
						description = <"*">
