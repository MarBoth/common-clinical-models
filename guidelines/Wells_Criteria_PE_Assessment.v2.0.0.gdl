	ontology = (GUIDE_ONTOLOGY) <
		term_definitions = <
			["en"] = (TERM_DEFINITION) <
				terms = <
					["gt0001"] = (TERM) <
						text = <"Wells' Criteria for PE Assessment">
						description = <"Wells' criteria quantifies an individual's risk of Pulmonary Embolism (PE). It guides physicians on the need for further testing (D-dimer or CT angiogram) to confirm if a suspected case is infact PE. It consists of 7 variables easily assessed at bedside, and the sum of individual scores from each variable ranges between 0 - 12.5. Score 0-1: Low Risk (1.3% risk prevalence), 2-6: Moderate Risk (16.2% risk prevalence) and score >6: High Risk (37.5% risk prevalence). Recommendations for further testing are also provided.">
					>
					["gt0005"] = (TERM) <
						text = <"Total score">
						description = <"The total score of each individual item is added and then halved (as the individual scores have been doubled to handle decimal numbers">
					>
					["gt0007"] = (TERM) <
						text = <"3-Tier Risk Level">
						description = <"3 Tier Risk levels: Low, Moderate or High">
					>
					["gt0008"] = (TERM) <
						text = <"2-Tier Risk Level">
						description = <"PE likely or unlikely">
					>
					["gt0009"] = (TERM) <
						text = <"Risk Prevalence">
						description = <"*">
					>
					["gt0011"] = (TERM) <
						text = <"Recommendation">
						description = <"Recommendation on whether to use d-dimer,high sensitivity dimer or CTA">
					>
					["gt0013"] = (TERM) <
						text = <"Set low risk, PE unlikely">
					>
					["gt0014"] = (TERM) <
						text = <"Set moderate risk, PE unlikely">
					>
					["gt0015"] = (TERM) <
						text = <"Set high risk, PE likely">
					>
					["gt0017"] = (TERM) <
						text = <"Set moderate risk, PE likely score">
					>
				>
			>
