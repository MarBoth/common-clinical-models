						description = <"*(en) This rule contains the expression logic for calculating the GAD-7 score.">
					>
					["gt0008"] = (TERM) <
						text = <"Känt mig nervös, orolig, spänd">
						description = <"*(en) Instantiates the EHR element Feeling nervous, anxious or on edge.">
					>
