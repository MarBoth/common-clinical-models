	ontology = (GUIDE_ONTOLOGY) <
		term_definitions = <
			["en"] = (TERM_DEFINITION) <
				terms = <
					["gt0001"] = (TERM) <
						text = <"Ritvo Autism and Asperger Diagnostic Scale 14 (RAADS-14) Assessment">
						description = <"Screening tool for autism spectrum disorder (ASD) in an adult psychiatric population. Consists of 14 self-administered questions derived from the 80-item Ritvo Autism and Asperger Diagnostic Scale-Revised (RAADS-R) and covers three domains: mentalizing deficits, social anxiety and sensory reactivity. Each question is scored 0, 1, 2 or 3 and the total score ranges from a minimum of 0 to a maximum of 42. The total score is assessed as: ≥31 = autism very likely; 14 – 30 = autism likely; 0 – 13 No autism symptoms (autism unlikely).">
					>
					["gt0003"] = (TERM) <
						text = <"Total score">
						description = <"RAADS-14 screen score">
					>
					["gt0005"] = (TERM) <
						text = <"RAADS-14 assessment">
						description = <"*">
					>
					["gt0006"] = (TERM) <
						text = <"Set autism unlikely">
					>
					["gt0007"] = (TERM) <
						text = <"Set autism likely">
					>
					["gt0008"] = (TERM) <
						text = <"Set autism very likely">
					>
				>
			>
			["sv"] = (TERM_DEFINITION) <
				terms = <
					["gt0001"] = (TERM) <
						text = <"Ritvo Autism and Asperger Diagnostic Scale 14 (RAADS-14) Utvärdering">
						description = <"Ritvo Autism and Asperger Diagnostic Scale 14 (RAADS-14) är en förkortad version av ursprungsformuläret (RAADS-R) och används som screening för att identifiera autismspektrumtillstånd (AST) hos vuxna. Det är ett patient självskattningsformulär med frågar inom tre katogorier: mentaliseringssvårigheter, social ångest och sensorisk reaktivitet. Skalan varierar mellan 0p (min) och 42p (max) och resultatet tolkas enligt: 0-13p - Autismspektrumtillstånd osannolikt, 14-30p - Möjligt autismspektrumtillstånd, >31p - Sannolikt autismspektrumtillstånd.">
					>
					["gt0003"] = (TERM) <
						text = <"Total poäng">
						description = <"*(en) RAADS-14 screen score">
					>
					["gt0005"] = (TERM) <
						text = <"RAADS-14 utvärdering">
						description = <"*(en) *">
					>
					["gt0006"] = (TERM) <
						text = <"Ange autismspektrumtillstånd osannolikt">
					>
					["gt0007"] = (TERM) <
						text = <"Ange möjligt autismspektrumtillstånd">
					>
					["gt0008"] = (TERM) <
						text = <"Ange sannolikt autismspektrumtillstånd">
					>
				>
			>
		>
	>
