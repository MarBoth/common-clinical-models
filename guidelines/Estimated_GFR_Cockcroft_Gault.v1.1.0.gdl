		pre_conditions = <"$gt0005.units=='kg'", "$gt0009.units=='umol/l'", "$gt0002.value<=($currentDateTime.value-18,a)">
		rules = <
			["gt0012"] = (RULE) <
				when = <"!fired($gt0012)", "$gt0003==local::at0005|Male|">
				then = <"$gt0011.magnitude=(((140-($currentDateTime.year-$gt0002.year))*$gt0005.magnitude)/(72*($gt0009.magnitude/88.42)))", "$gt0011.precision=1", "$gt0011.units='ml/min'">
				priority = <2>
			>
			["gt0013"] = (RULE) <
				when = <"!fired($gt0013)", "$gt0003==local::at0006|Female|">
				then = <"$gt0011.units='ml/min'", "$gt0011.precision=1", "$gt0011.magnitude=((((140-($currentDateTime.year-$gt0002.year))*$gt0005.magnitude)*0.85)/(72*($gt0009.magnitude/88.42)))">
				priority = <1>
			>
		>
	>
